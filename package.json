{
  "version": "0.1.0",
  "description": "&_&",
  "main": "src/main.ts",
  "license": "MIT",
  "workspaces": [
    "./services/api",
    "./services/client"
  ],
  "scripts": {
    "start:full": "concurrently \"npm run dev:client\" \"npm run dev:api\"",
    "dev:full": "concurrently \"npm run dev:client\" \"npm run dev:api\"",
    "start:client": "cd ./services/client && npm run start",
    "start:api": "cd ./services/api && npm run start",
    "dev:client": "cd ./services/client && npm run dev",
    "dev:api": "cd ./services/api && npm run dev",
    "test:client": "cd ./services/client && npm run test",
    "test:api": "cd ./services/api && npm run test"
  },
  "devDependencies": {
    "@types/concurrently": "^6.4.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "typescript": "^4.5.2"
  }
}
